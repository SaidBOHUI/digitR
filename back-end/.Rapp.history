install.packages("shiny")
library(shiny)
library(shiny)#
#
# Interface utilisateur#
ui <- fluidPage(#
  titlePanel("Bonjour Shiny !"),#
  sidebarLayout(#
    sidebarPanel(#
      sliderInput("obs", "Nombre d'observations:", min = 1, max = 100, value = 50)#
    ),#
    mainPanel(#
      plotOutput("distPlot")#
    )#
  )#
)#
#
# Logique serveur#
server <- function(input, output) {#
  output$distPlot <- renderPlot({#
    hist(rnorm(input$obs))#
  })#
}#
#
# Lancer l'application Shiny#
shinyApp(ui = ui, server = server)
